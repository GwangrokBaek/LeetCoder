{
  "name": "leetup",
  "publisher": "GwangrokBaek",
  "displayName": "LeetUp",
  "description": "vscode extension for leetcode",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.64.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onView:nodeDependencies"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "views": {
      "leetup": [
        {
          "id": "profile",
          "name": "Profile"
        },
        {
          "id": "dailyChallenge",
          "name": "Daily Challenge",
          "when": "false"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "leetup",
          "title": "LeetUp",
          "icon": "assets/image/dep.svg"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "profile",
        "contents": "To use LeetUp, please login with your [leetcode](https://leetcode.com/) account.\n[Login](command:leetup.login)",
        "when": "true"
      }
    ],
    "commands": [
      {
        "command": "leetup.submit",
        "title": "LeetCode Submit"
      },
      {
        "command": "leetup.login",
        "title": "LeetCode Login"
      },
      {
        "command": "leetup.logout",
        "title": "LeetCode Logout"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.64.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "14.x",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "eslint": "^8.9.0",
    "glob": "^7.2.0",
    "mocha": "^9.2.1",
    "typescript": "^4.5.5",
    "@vscode/test-electron": "^2.1.2"
  }
}
